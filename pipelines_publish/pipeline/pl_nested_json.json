{
	"name": "pl_nested_json",
	"properties": {
		"activities": [
			{
				"name": "Copy Nested Json File to Tbl",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['employeeId']"
								},
								"sink": {
									"name": "EmployeeId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['name']['first']"
								},
								"sink": {
									"name": "FirstName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['name']['last']"
								},
								"sink": {
									"name": "LastName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['contact']['email']"
								},
								"sink": {
									"name": "Email",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['contact']['address']['line1']"
								},
								"sink": {
									"name": "AddressLine1",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['contact']['address']['line2']"
								},
								"sink": {
									"name": "AddressLine2",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['contact']['address']['city']"
								},
								"sink": {
									"name": "City",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['contact']['address']['pincode']"
								},
								"sink": {
									"name": "Pincode",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['audit']['createdOn']"
								},
								"sink": {
									"name": "CreatedOn",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['audit']['modifiedOn']"
								},
								"sink": {
									"name": "ModifiedOn",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['audit']['createdBy']"
								},
								"sink": {
									"name": "CreatedBy",
									"type": "String"
								}
							}
						],
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "src_nested_jsonFile_sqlTbl",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "snk_nested_jsonFile_sqlTbl",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "yachendradev"
		},
		"annotations": [],
		"lastPublishTime": "2025-12-04T05:37:00Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}